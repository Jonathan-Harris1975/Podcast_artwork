# OpenAI
OPENAI_API_KEY=sk-...

# Cloudflare R2 S3-compatible settings
R2_ACCESS_KEY_ID=R2_ACCESS_KEY
R2_SECRET_ACCESS_KEY=R2_SECRET
R2_BUCKET_NAME=my-bucket
R2_ENDPOINT=https://<accountid>.r2.cloudflarestorage.com

# Public base url used to build public link (must be configured in your Cloudflare setup)
# Example: https://<accountid>.r2.dev/<bucket>
R2_PUBLIC_BASE_URL=https://pub-<accountid>.r2.dev/my-bucket

PORT=3000
